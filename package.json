{
  "name": "assignment",
  "version": "1.0.0",
  "description": "Assignment project, with parcel used as build tool",
  "author": "Morten Broesby-Olsen <mortenbroesby@gmail.com>",
  "main": "index.ts",
  "scripts": {
    "eslint": "eslint --fix --ext .js,.vue src",
    "dev": "parcel serve src/index.html -d dist --public-url /dist/ --open",
    "build": "cross-env NODE_ENV=production parcel build src/index.html -d build --public-url ./src --out-dir ./dist",
    "clean": "rimraf .cache/ && rimraf dist/ && rimraf build/"
  },
  "keywords": [
    "vue",
    "typescript",
    "parcel",
    "scss"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/node": "^12.0.8",
    "autoprefixer": "^9.1.3",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "js-logger": "^1.6.0",
    "password-hash": "^1.2.2",
    "postcss-modules": "^1.3.2",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.5.0",
    "eslint-plugin-vue": "^4.7.1",
    "parcel-bundler": "^1.9.7",
    "sass": "^1.21.0",
    "vue-template-compiler": "^2.5.17",
    "cross-env": "^5.1.3",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.2"
  }
}
